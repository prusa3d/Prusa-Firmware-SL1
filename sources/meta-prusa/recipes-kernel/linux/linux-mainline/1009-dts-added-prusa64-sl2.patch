From 33b9f89b1c3def6dd8e8b011415edd76792d223b Mon Sep 17 00:00:00 2001
From: Roman Beranek <roman.beranek@prusa3d.com>
Date: Thu, 18 Mar 2021 09:57:07 +0100
Subject: [PATCH] dts: added prusa64-sl2

---
 arch/arm64/boot/dts/allwinner/Makefile        |   1 +
 .../dts/allwinner/sun50i-a64-prusa64-sl2.dts  | 535 ++++++++++++++++++
 2 files changed, 536 insertions(+)
 create mode 100644 arch/arm64/boot/dts/allwinner/sun50i-a64-prusa64-sl2.dts

diff --git a/arch/arm64/boot/dts/allwinner/Makefile b/arch/arm64/boot/dts/allwinner/Makefile
index 02cbef06653c..d4337d2792ad 100644
--- a/arch/arm64/boot/dts/allwinner/Makefile
+++ b/arch/arm64/boot/dts/allwinner/Makefile
@@ -15,6 +15,7 @@ dtb-$(CONFIG_ARCH_SUNXI) += sun50i-a64-pinephone-1.2.dtb
 dtb-$(CONFIG_ARCH_SUNXI) += sun50i-a64-pinetab.dtb
 dtb-$(CONFIG_ARCH_SUNXI) += sun50i-a64-pinetab-early-adopter.dtb
 dtb-$(CONFIG_ARCH_SUNXI) += sun50i-a64-prusa64-sl1.dtb
+dtb-$(CONFIG_ARCH_SUNXI) += sun50i-a64-prusa64-sl2.dtb
 dtb-$(CONFIG_ARCH_SUNXI) += sun50i-a64-sopine-baseboard.dtb
 dtb-$(CONFIG_ARCH_SUNXI) += sun50i-a64-teres-i.dtb
 dtb-$(CONFIG_ARCH_SUNXI) += sun50i-a100-allwinner-perf1.dtb
diff --git a/arch/arm64/boot/dts/allwinner/sun50i-a64-prusa64-sl2.dts b/arch/arm64/boot/dts/allwinner/sun50i-a64-prusa64-sl2.dts
new file mode 100644
index 000000000000..7fa28bd5f9cb
--- /dev/null
+++ b/arch/arm64/boot/dts/allwinner/sun50i-a64-prusa64-sl2.dts
@@ -0,0 +1,528 @@
+/*
+ * Copyright (C) 2021 Roman Beranek <roman.beranek@prusa3d.com>
+ *
+ * SPDX-License-Identifier: (GPL-2.0 OR MIT)
+ */
+
+/dts-v1/;
+
+#include "sun50i-a64.dtsi"
+#include "sun50i-a64-cpu-opp.dtsi"
+
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/pwm/pwm.h>
+
+/ {
+	model = "Prusa64-SL2";
+	compatible = "prusa,prusa64-sl2", "allwinner,sun50i-a64";
+
+	aliases {
+		mmc0 = &mmc0;
+		mmc1 = &mmc1;
+		mmc2 = &mmc2;
+		serial0 = &uart0;
+		serial1 = &r_uart;
+		serial2 = &uart2;
+	};
+
+	chosen {
+		stdout-path = "serial0:115200n8";
+	};
+
+	connector {
+		compatible = "hdmi-connector";
+		type = "a";
+
+		port {
+			hdmi_con_in: endpoint {
+				remote-endpoint = <&hdmi_out_con>;
+			};
+		};
+	};
+
+	reg_avdd_lcd: avdd-lcd {
+		compatible = "regulator-fixed";
+		regulator-name = "dsi1-lcd-avdd";
+		regulator-min-microvolt = <3300000>;
+		regulator-max-microvolt = <3300000>;
+		enable-active-high;
+		gpio = <&r_pio 0 4 GPIO_ACTIVE_HIGH>; /* PL4 */
+	};
+
+	reg_usb1_vbus: usb1-vbus {
+		compatible = "regulator-fixed";
+		regulator-name = "usb1-vbus";
+		regulator-min-microvolt = <5000000>;
+		regulator-max-microvolt = <5000000>;
+		regulator-boot-on;
+		enable-active-high;
+		gpio = <&pio 6 9 GPIO_ACTIVE_HIGH>; /* PG9 */
+		status = "okay";
+	};
+
+	reserved-memory {
+		#address-cells = <1>;
+		#size-cells = <1>;
+		ranges;
+
+		linux,cma {
+			compatible = "shared-dma-pool";
+			reusable;
+			size = <0x10000000>;
+			linux,cma-default;
+		};
+	};
+
+	mux: mux-controller {
+		compatible = "gpio-mux";
+		#mux-control-cells = <0>;
+
+		mux-gpios = <&pio 4 12 GPIO_ACTIVE_HIGH>, /* PE12 */
+			    <&pio 6  6 GPIO_ACTIVE_HIGH>, /* PG6  */
+			    <&pio 4  6 GPIO_ACTIVE_HIGH>, /* PE6  */
+			    <&pio 3  3 GPIO_ACTIVE_HIGH>; /* PD3  */
+	};
+
+	adc-mux {
+		compatible = "io-channel-mux";
+		io-channels = <&adc>;
+		io-channels-names = "parent";
+
+		mux-controls = <&mux>;
+
+		channels = "led", "vin", "battery", "adc3", "adc4",
+			   "adc5", "adc6", "temp_ob", "dc1_current",
+			   "tenso_temp", "tenso_light", "temp3",
+			   "temp2", "temp1", "current_meas", "heater";
+	};
+
+	codec_dummy: codec-dummy {
+		#sound-dai-cells = <0>;
+		compatible = "linux,snd-soc-dummy";
+		status = "okay";
+	};
+
+	sound_test {
+		compatible = "simple-audio-card";
+		simple-audio-card,name = "On-board I2S";
+		simple-audio-card,format = "dsp_b";
+		simple-audio-card,bitclock-master = <&cpu_i2s0>;
+		simple-audio-card,frame-master = <&cpu_i2s0>;
+		simple-audio-card,mclk-fs = <128>;
+		status = "okay";
+
+		cpu_i2s0: simple-audio-card,cpu {
+			sound-dai = <&i2s0>;
+		};
+
+		simple-audio-card,codec {
+			sound-dai = <&codec_dummy>;
+		};
+	};
+
+	wifi_pwrseq: wifi_pwrseq {
+		compatible = "mmc-pwrseq-simple";
+		reset-gpios = <&pio 7 11 GPIO_ACTIVE_LOW>; /* PH11 */
+	};
+};
+
+&pio {
+	clocks = <&ccu CLK_BUS_PIO>, <&osc24M>, <&osc32k>;
+	clock-names = "apb", "hosc", "losc";
+	input-debounce = <1 0 0>;
+
+	edtint_pins: edtint-pins {
+		pins = "PB2";
+		function = "gpio_in";
+		bias-pull-up;
+	};
+
+	i2s0_pins: i2s0-pins {
+		pins = "PB4", "PB5", "PB6", "PB7";
+		function = "i2s0";
+	};
+
+};
+
+&codec {
+	status = "okay";
+};
+
+&codec_analog {
+	cpvdd-supply = <&reg_eldo1>;
+	status = "okay";
+};
+
+&cpu0 {
+	cpu-supply = <&reg_dcdc2>;
+};
+
+&cpu1 {
+	cpu-supply = <&reg_dcdc2>;
+};
+
+&cpu2 {
+	cpu-supply = <&reg_dcdc2>;
+};
+
+&cpu3 {
+	cpu-supply = <&reg_dcdc2>;
+};
+
+&dai {
+	status = "okay";
+};
+
+&de {
+	status = "okay";
+};
+
+&dphy {
+	status = "okay";
+};
+
+&dsi {
+	vcc-dsi-supply = <&reg_dldo2>;
+	status = "disable";
+
+	panel@0 {
+		compatible = "tdo,tl040wv27ct3";
+		reg = <0>;
+		reset-gpios = <&pio 3 8 GPIO_ACTIVE_LOW>; /* PD8 */
+		power-supply = <&reg_dldo2>;
+		use-dsi-backlight;
+	};
+};
+
+&emac {
+	pinctrl-names = "default";
+	pinctrl-0 = <&rmii_pins>;
+	phy-mode = "rmii";
+	phy-handle = <&ext_rmii_phy>;
+	phy-supply = <&reg_dcdc1>;
+	status = "okay";
+
+	nvmem-cells = <&ethaddr>;
+	nvmem-cell-names = "mac-address";
+};
+
+&mdio {
+	ext_rmii_phy: ethernet-phy@1 {
+		compatible = "ethernet-phy-ieee802.3-c22";
+		reg = <1>;
+	};
+};
+
+&hdmi {
+	hvcc-supply = <&reg_dldo1>;
+	status = "okay";
+};
+
+&hdmi_out {
+	hdmi_out_con: endpoint {
+		remote-endpoint = <&hdmi_con_in>;
+	};
+};
+
+&i2c0 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&i2c0_pins>;
+	status = "okay";
+
+	/* EEPROM bank and serial number are treated as separate devices */
+	at24c02@50 { compatible = "atmel,24c02"; reg = <0x50>; };
+	at24cs02@58 { compatible = "atmel,24cs02"; reg = <0x58>; };
+
+	adc: mcp3221@4d {
+		compatible = "microchip,mcp3221";
+		reg = <0x4d>;
+		reference-voltage-microvolt = <5000000>; /* 5.0 V */
+		#io-channel-cells = <0>;
+	};
+
+	pcf8563@51 {
+		compatible = "nxp,pcf8563";
+		reg = <0x51>;
+		#clock-cells = <0>;
+	};
+};
+
+&i2c1 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&i2c1_pins &edtint_pins>;
+	status = "okay";
+
+	tc358870: hdmi-bridge@f {
+		compatible = "toshiba,tc358870";
+		reg = <0x0f>;
+		vdd12_mipi-supply = <&reg_fldo1>;
+		vdd11_hdmi-supply = <&reg_dcdc4>;
+		avdd_lcd-supply = <&reg_avdd_lcd>;
+		iovdd_lcd-supply = <&reg_eldo2>;
+		tc358870-reset-gpios = <&pio 3 6 GPIO_ACTIVE_LOW>; /* PD6 */
+		lcd-reset-gpios = <&pio 3 0 GPIO_ACTIVE_LOW>;      /* PD0 */
+		hdmi-bridge = <&hdmi>;
+	};
+
+	edt-ft5x06@38 {
+		status = "okay";
+		compatible = "focaltech,ft6236", "edt,edt-ft5x06";
+		reg = <0x38>;
+		interrupt-parent = <&pio>;
+		interrupts = <1 2 IRQ_TYPE_EDGE_FALLING>; /* PB2 */
+		reset-gpios = <&pio 1 3 GPIO_ACTIVE_LOW>; /* PB3 */
+		touchscreen-size-x = <480>;
+		touchscreen-size-y = <800>;
+		touchscreen-min-x = <0>;
+		touchscreen-min-y = <0>;
+		touchscreen-x-mm = <56>;
+		touchscreen-y-mm = <94>;
+		vcc-supply = <&reg_dldo3>;
+	};
+};
+
+&i2c2 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&i2c2_pins>;
+	status = "okay";
+
+	/* EEPROM bank and serial number are treated as separate devices */
+	at24c02@50 { compatible = "atmel,24c02"; reg = <0x50>; };
+	at24cs02@58 { compatible = "atmel,24cs02"; reg = <0x58>; };
+};
+
+&i2s0 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&i2s0_pins>;
+	status = "okay";
+};
+
+
+&mmc0 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&mmc0_pins>;
+	vmmc-supply = <&reg_dcdc1>;
+	cd-gpios = <&pio 5 6 GPIO_ACTIVE_LOW>;
+	disable-wp;
+	bus-width = <4>;
+	status = "okay";
+};
+
+&mmc1 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&mmc1_pins>;
+	vmmc-supply = <&reg_dldo4>;
+	vqmmc-supply = <&reg_dldo4>;
+	mmc-pwrseq = <&wifi_pwrseq>;
+	bus-width = <4>;
+	non-removable;
+	status = "okay";
+
+	brcmf: wifi@1 {
+		reg = <1>;
+		compatible = "brcm,bcm4329-fmac";
+		interrupt-parent = <&pio>;
+		interrupts = <3 6 IRQ_TYPE_LEVEL_LOW>; /* PD6 */
+		interrupt-names = "host-wake";
+	};
+};
+
+&mmc2 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&mmc2_pins>;
+	vmmc-supply = <&reg_dcdc1>;
+	bus-width = <8>;
+	non-removable;
+	cap-mmc-hw-reset;
+	status = "okay";
+};
+
+&r_rsb {
+	status = "okay";
+
+	axp803: pmic@3a3 {
+		compatible = "x-powers,axp803";
+		reg = <0x3a3>;
+		interrupt-parent = <&r_intc>;
+		interrupts = <0 IRQ_TYPE_LEVEL_LOW>;
+		x-powers,drive-vbus-en;	/* set N_VBUSEN as output pin */
+	};
+};
+
+#include "axp803.dtsi"
+
+&ac_power_supply {
+	status = "okay";
+};
+
+&reg_aldo1 {
+	regulator-always-on;
+	regulator-min-microvolt = <2800000>;
+	regulator-max-microvolt = <2800000>;
+	regulator-name = "vcc-pe";
+};
+
+&reg_aldo2 {
+	regulator-always-on;
+	regulator-min-microvolt = <3300000>;
+	regulator-max-microvolt = <3300000>;
+	regulator-name = "vcc-pl";
+};
+
+&reg_aldo3 {
+	regulator-always-on;
+	regulator-min-microvolt = <3000000>;
+	regulator-max-microvolt = <3000000>;
+	regulator-name = "vcc-pll-avcc";
+};
+
+&reg_dcdc1 {
+	regulator-always-on;
+	regulator-min-microvolt = <3300000>;
+	regulator-max-microvolt = <3300000>;
+	regulator-name = "vcc-3v3";
+};
+
+&reg_dcdc2 {
+	regulator-always-on;
+	regulator-min-microvolt = <1040000>;
+	regulator-max-microvolt = <1300000>;
+	regulator-name = "vdd-cpux";
+};
+
+/* DCDC3 is polyphased with DCDC2 */
+
+&reg_dcdc4 {
+	regulator-min-microvolt = <1150000>;
+	regulator-max-microvolt = <1150000>;
+	regulator-name = "dsi1-1.15V-hdmi";
+};
+
+&reg_dcdc5 {
+	regulator-always-on;
+	regulator-min-microvolt = <1200000>;
+	regulator-max-microvolt = <1200000>;
+	regulator-name = "vdd2-lpddr";
+};
+
+&reg_dcdc6 {
+	regulator-always-on;
+	regulator-min-microvolt = <1100000>;
+	regulator-max-microvolt = <1100000>;
+	regulator-name = "vdd-sys";
+};
+
+&reg_dldo1 {
+	regulator-always-on;
+	regulator-min-microvolt = <3300000>;
+	regulator-max-microvolt = <3300000>;
+	regulator-name = "vcc-hdmi";
+};
+
+&reg_dldo2 {
+	regulator-min-microvolt = <3300000>;
+	regulator-max-microvolt = <3300000>;
+	regulator-name = "dsi0-3.3v-mipi";
+};
+
+&reg_dldo3 {
+	regulator-min-microvolt = <3300000>;
+	regulator-max-microvolt = <3300000>;
+	regulator-name = "dsi0-3.3v-ctp";
+};
+
+&reg_drivevbus {
+	regulator-name = "usb0-vbus";
+	status = "okay";
+};
+
+&reg_eldo1 {
+	// TODO: make sound system use this regulator
+	regulator-always-on;
+
+	regulator-min-microvolt = <1800000>;
+	regulator-max-microvolt = <1800000>;
+	regulator-name = "cpvdd";
+};
+
+&reg_eldo2 {
+	regulator-min-microvolt = <1800000>;
+	regulator-max-microvolt = <1800000>;
+	regulator-name = "dsi1-1.8v";
+};
+
+&reg_fldo1 {
+	regulator-min-microvolt = <1200000>;
+	regulator-max-microvolt = <1200000>;
+	regulator-name = "1.2v-hsic";
+};
+
+&reg_fldo2 {
+	regulator-always-on;
+	regulator-min-microvolt = <1100000>;
+	regulator-max-microvolt = <1100000>;
+	regulator-name = "vdd-cpus";
+};
+
+&reg_rtc_ldo {
+	regulator-name = "vcc-rtc";
+};
+
+&sid {
+	ethaddr: ethaddr@18 {
+		reg = <0x18 0x06>;
+	};
+	sn: sn@3c {
+		reg = <0x3c 0x06>;
+	};
+};
+
+&uart0 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart0_pb_pins>;
+	status = "okay";
+};
+
+&uart2 {
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart2_pins>;
+	status = "okay";
+};
+
+&ehci0 {
+	status = "okay";
+};
+
+&ehci1 {
+	status = "okay";
+};
+
+&ohci0 {
+	status = "okay";
+};
+
+&ohci1 {
+	status = "okay";
+};
+
+&usb_otg {
+	dr_mode = "host";
+	status = "okay";
+};
+
+&usbphy {
+	status = "okay";
+	usb0_id_det-gpios = <&pio 7 9 GPIO_ACTIVE_HIGH>; /* PH9 */
+	usb0_vbus-supply = <&reg_drivevbus>;
+	usb1_vbus-supply = <&reg_usb1_vbus>;
+};
+
+&r_pwm {
+	status = "okay";
+};
+
+&r_uart {
+       pinctrl-names = "default";
+       pinctrl-0 = <&r_uart_pins>;
+       status = "okay";
+};
-- 
2.32.0

